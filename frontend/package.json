{
  "name": "eflaweb-frontend",
  "version": "1.1.0",
  "authors": [
    "Hannes Leutloff <hannes.leutloff@aol.de>",
    "Noah Hummel <strangedev@posteo.net>"
  ],
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production gulp build",
    "dev": "NODE_ENV=development gulp dev",
    "test": "jest && codecov"
  },
  "dependencies": {
    "abortcontroller-polyfill": "^1.1.9",
    "chart.js": "^2.7.2",
    "esm": "^3.0.84",
    "express": "^4.16.3",
    "fluture": "^10.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "morgan": "^1.9.1",
    "node-fetch": "^2.2.0",
    "ramda": "^0.26.0",
    "v-click-outside": "^2.0.1",
    "vue": "^2.5.17",
    "vue-js-modal": "^1.3.25",
    "vue-notification": "^1.3.13",
    "vue-router": "^3.0.1",
    "vuex-fluture": "^1.0.5-fluture"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "codecov": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^2.0.0",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-vue": "^5.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "greenkeeper-lockfile": "^1.15.1",
    "gulp": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.7.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.0",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.3",
    "regenerator-runtime": "^0.13.0",
    "rimraf": "^2.6.0",
    "sass-loader": "^7.1.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-svg-loader": "^0.10.0",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.19.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 9.0.0",
    "npm": ">= 5.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,vue}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "plugin:prettier/recommended",
      "eslint:recommended"
    ],
    "globals": {
      "ltiSessionToken": true,
      "ltiLaunchParameters": true
    },
    "plugins": [
      "jest"
    ],
    "env": {
      "jest/globals": true
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
